    "import matplotlib.pyplot as plt\n",
    "\n",
    "tottenham_filter = (combined['team'] == 'Tottenham Hotspur') | (combined['opponent'] == 'Tottenham')\n",
    "tottenham_results = combined[tottenham_filter].copy()\n",
    "\n",
    "tottenham_results['date'] = tottenham_results['date'].dt.strftime('%Y-%m-%d')\n",
    "\n",
    "display_columns = ['date', 'team', 'opponent', 'result', 'actual', 'predicted']\n",
    "tottenham_to_display = tottenham_results[display_columns]\n",
    "\n",
    "display(tottenham_to_display)"
